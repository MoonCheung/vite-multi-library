{
  "name": "@img-uploader/react",
  "private": false,
  "version": "1.0.0",
  "description": "@img-uploader/react",
  "type": "module",
  "main": "dist/index.react.cjs",
  "module": "dist/index.react.js",
  "types": "dist/index.react.d.ts",
  "unpkg": "dist/index.react.global.js",
  "jsdelivr": "dist/index.react.global.js",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "rollup -wc rollup.config.ts --configPlugin typescript"
  },
  "exports": {
    ".": {
      "types": "./dist/index.react.d.ts",
      "import": "./dist/index.react.js",
      "require": "./dist/index.react.cjs"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MoonCheung/img-uploader",
    "directory": "packages/react"
  },
  "dependencies": {
    "@img-uploader/core": "workspace:^1.0.0",
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.11",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}
