{
  "name": "@img-uploader/core",
  "private": true,
  "version": "1.0.0",
  "description": "@img-uploader/core",
  "type": "module",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "unpkg": "dist/index.global.js",
  "jsdelivr": "dist/index.global.js",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "rollup -wc rollup.config.ts --configPlugin typescript"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MoonCheung/img-uploader",
    "directory": "packages/core"
  }
}
