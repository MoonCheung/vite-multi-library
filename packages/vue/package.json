{
  "name": "@img-uploader/vue",
  "private": false,
  "version": "1.0.0",
  "description": "@img-uploader/vue",
  "type": "module",
  "main": "dist/index.vue.cjs",
  "module": "dist/index.vue.js",
  "types": "dist/index.vue.d.ts",
  "unpkg": "dist/index.vue.global.js",
  "jsdelivr": "dist/index.vue.global.js",
  "scripts": {
    "build": "rollup -c rollup.config.ts --configPlugin typescript",
    "watch": "rollup -wc rollup.config.ts --configPlugin typescript"
  },
  "exports": {
    ".": {
      "types": "./dist/index.vue.d.ts",
      "import": "./dist/index.vue.js",
      "require": "./dist/index.vue.cjs"
    },
    "./*": "./*"
  },
  "sideEffects": false,
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/MoonCheung/img-uploader",
    "directory": "packages/vue"
  },
  "dependencies": {
    "@img-uploader/core": "workspace:^1.0.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@vue/babel-plugin-jsx": "^1.1.1"
  }
}
